#
# Listener for domain <%= @name %>
#
<%= @listen_protocol %>
    Address <%= @listen_ip %>
    Port    <%= @listen_port %>
    <% if @listen_protocol == "ListenHTTPS" %>
    Cert    "<%= @server_cert -%>"
    <% end %>
    Service
        HeadRequire "<%= @head_require -%>"
	<% @backend_ips.each do |this_ip| -%>
        BackEnd
            Address <%= this_ip %>
            Port    <%= @backend_port %>
        End
	<% end -%>
    End
End

