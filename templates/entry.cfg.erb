#
# Listener for domain <%=@name%>
#
<%=@listen_protocol%>
    Address <%=@listen_ip%>
    Port    <%=@listen_port%>
    <% if @listen_protocol == "ListenHTTPS"%>
    <% @certs.each do |cert| -%> 
    Cert    "<%= cert %>"
    Ciphers "ECDHE-RSA-AES128-SHA256:AES128-GCM-SHA256:RC4:HIGH:!MD5:!aNULL:!EDH"
    <% end %>
    <% end %>
    Service
        HeadRequire "<%=@head_require-%>"
        BackEnd
            Address <%=@backend_ip%>
            Port    <%=@backend_port%>
        End
    End
End

