#
# Listener for domain <%=name%>
#
<%=listen_protocol%>
    Address <%=listen_ip%>
    Port    <%=listen_port%>
    #Cert    "/etc/ssl/certs/server.crt"
    Service
        HeadRequire "<%=head_require-%>"
        BackEnd
            Address <%=backend_ip%>
            Port    <%=backend_port%>
        End
    End

