#
# Listener for domain <%=@name%>
#
<%=@listen_protocol%>
    Address <%=@listen_ip%>
    Port    <%=@listen_port%>
    <% if @listen_protocol == "ListenHTTPS"%>
    Cert    "<%=@cert%>"
    <% end %>
    Service
        HeadRequire "<%=@head_require-%>"
        BackEnd
            Address <%=@backend_ip%>
            Port    <%=@backend_port%>
        End
    End
End

